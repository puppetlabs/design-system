{
  "name": "@puppet/react-components",
  "version": "6.0.0-alpha.13",
  "author": "Puppet, Inc.",
  "license": "Apache-2.0",
  "main": "build/library.js",
  "scripts": {
    "link": "npm link",
    "analyze": "webpack --config webpack.analyze.config.js",
    "build": "NODE_ENV=production webpack",
    "prepare": "npm run build",
    "complexity": "es6-plato -r -d complexity source/react && open ./complexity/index.html",
    "watch": "nodemon --watch source -x \"npm run build\"",
    "test": "nyc --require=./test/jsdom.js --reporter=html mocha --require @babel/register --recursive",
    "lint": "npm run js-lint && npm run scss-lint",
    "js-lint": "eslint --quiet 'source/react/**/*.js'",
    "scss-lint": "stylelint 'source/scss/**/*.scss'",
    "format": "eslint --fix 'source/**/*.{js,jsx}'; prettier --write 'source/**/*.{js,jsx,json,md}'",
    "start": "cd ../design-system-website && npm start",
    "upgrade": "npx npm-check-updates --upgrade"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-preset-react-hmre": "^1.1.1",
    "caniuse-lite": "^1.0.30001243",
    "chai": "^4.3.4",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "^1.0.0-rc.10",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "es6-plato": "^1.2.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.3.0",
    "jsdom": "^15.2.1",
    "mocha": "^6.2.3",
    "mocha-jsdom": "^2.0.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "nyc": "^14.1.1",
    "prettier": "^1.19.1",
    "react": "^16.14.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.14.0",
    "react-router": "^5.2.0",
    "resolve-url-loader": "^3.1.4",
    "sass-loader": "^8.0.2",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.7.0",
    "stylelint": "^11.1.1",
    "stylelint-config-sass-guidelines": "^6.2.0",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "@puppet/sass-variables": "^2.0.0-alpha.2",
    "classnames": "^2.3.1",
    "hoist-non-react-statics": "^3.3.1",
    "prop-types": "^15.7.2",
    "react-modal": "^3.14.3",
    "react-popper": "^2.2.5",
    "react-transition-group": "^4.4.2",
    "scroll-into-view-if-needed": "^2.2.28"
  }
}
