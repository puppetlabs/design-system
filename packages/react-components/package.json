{
  "name": "@puppet/react-components",
  "version": "5.9.3",
  "author": "Puppet, Inc.",
  "license": "UNLICENSED",
  "main": "build/library.js",
  "scripts": {
    "analyze": "webpack --config webpack.analyze.config.js",
    "prebuild": "rimraf build",
    "build": "NODE_ENV=production webpack",
    "prepare": "npm run build",
    "complexity": "es6-plato -r -d complexity source/react && open ./complexity/index.html",
    "watch": "nodemon --watch source -x \"npm run build\"",
    "test": "nyc --require=./test/jsdom.js --reporter=html mocha --require @babel/register --require core-js --require regenerator-runtime/runtime --recursive",
    "lint": "npm run js-lint && npm run scss-lint",
    "js-lint": "eslint --quiet 'source/react/**/*.js'",
    "scss-lint": "stylelint 'source/scss/**/*.scss'",
    "format": "eslint --fix 'source/**/*.{js,jsx}'; prettier --write 'source/**/*.{js,jsx,json,md}'",
    "start": "cd ../design-system-website && styleguidist server"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "es6-plato": "^1.1.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fast-csv": "^3.0.1",
    "file-loader": "^4.0.0",
    "jsdom": "^15.1.1",
    "mocha": "^6.1.4",
    "mocha-jsdom": "^2.0.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "react-addons-test-utils": "^15.6.2",
    "react-router": "^5.0.1",
    "react-styleguidist": "^9.1.16",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "stylelint": "^10.1.0",
    "stylelint-config-sass-guidelines": "^6.0.0",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@puppet/sass-variables": "^1.1.1",
    "classnames": "^2.2.6",
    "core-js": "^3.1.4",
    "hoist-non-react-statics": "^3.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-modal": "^3.8.1",
    "react-transition-group": "^4.1.1",
    "regenerator-runtime": "^0.13.3",
    "scroll-into-view-if-needed": "^2.2.20"
  }
}
